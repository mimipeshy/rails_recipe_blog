 <% if current_user %>
        <% pre_like = @article.votes.find { |vote| vote.user_id == current_user.id} %>
        <% if pre_like %>
        <i class="fa fa-thumbs-down" aria-hidden="true"><%= button_to 'Unlike', article_vote_path(@article, pre_like), method: :delete %></i>
        <% else %>
        <i class="fa fa-thumbs-up" aria-hidden="true"><%= button_to 'Like', article_votes_path(@article), method: :post %></i>
      <% end %>
      <% end %>
